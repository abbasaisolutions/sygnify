{
  "name": "sygnify-backend",
  "version": "2.0.0",
  "description": "Advanced analytics backend with AI-powered data comprehension and intelligent insights",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "echo 'No build step required for Node.js backend'",
    "test": "mocha server/tests/**/*.js --timeout 10000",
    "test:watch": "mocha server/tests/**/*.js --watch --timeout 10000",
    "test:llama": "node tests/llama-test.js",
    "test:comprehensive": "node tests/comprehensive-tests.js",
    "test:consistency": "node tests/consistency-test.js",
    "demo:narrative": "node tests/narrative-demo.js",
    "lint": "eslint server/ --ext .js",
    "lint:fix": "eslint server/ --ext .js --fix",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "health": "curl http://localhost:3000/api/v1/health",
    "clean": "rm -rf uploads/* logs/*.log",
    "backup": "pg_dump -h localhost -U sygnify_user sygnify_analytics > backup_$(date +%Y%m%d_%H%M%S).sql",
    "restore": "echo 'Usage: psql -h localhost -U sygnify_user sygnify_analytics < backup_file.sql'"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.2.0",
    "bull": "^4.10.2",
    "cors": "^2.8.5",
    "csv-parse": "^6.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^3.1.0",
    "multer": "^2.0.1",
    "natural": "^8.1.0",
    "papaparse": "^5.4.1",
    "pg": "^8.16.3",
    "redis": "^4.6.12",
    "simple-statistics": "^7.8.8",
    "sqlite3": "^5.1.7",
    "stripe": "^14.2.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "chai": "^5.2.1",
    "eslint": "^8.56.0",
    "mocha": "^11.7.1",
    "nodemon": "^3.0.2",
    "sinon": "^21.0.0",
    "supertest": "^7.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "analytics",
    "backend",
    "api",
    "nodejs",
    "express",
    "postgresql",
    "redis",
    "ai"
  ],
  "author": "Sygnify Analytics Hub Team",
  "license": "MIT"
}
